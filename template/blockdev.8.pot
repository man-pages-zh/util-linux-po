#. type: TH
#: raw/man8/blockdev.8:5
#, no-wrap
msgid "BLOCKDEV"
msgstr ""

#. type: TH
#: raw/man8/blockdev.8:5
#, no-wrap
msgid "August 2010"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:8
msgid "blockdev - call block device ioctls from the command line"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:16
msgid ""
"B<blockdev> [B<-q>] [B<-v>] I<command> [I<command>\\&...\\&] I<device> "
"[I<device>\\&...\\&]"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:20
msgid "B<blockdev> B<--report> [I<device>\\&...\\&]"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:24
msgid ""
"The utility B<blockdev> allows one to call block device ioctls from the "
"command line."
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:27
msgid "Print version and exit."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:27
#, no-wrap
msgid "B<-q>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:29
msgid "Be quiet."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:31
#, no-wrap
msgid "B<--report>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:35
msgid ""
"Print a report for the specified device. It is possible to give multiple "
"devices. If none is given, all devices which appear in /proc/partitions are "
"shown. Note that the partition StartSec is in 512-byte sectors."
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:37
msgid "It is possible to give multiple devices and multiple commands."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:37
#, no-wrap
msgid "B<--flushbufs>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:39
msgid "Flush buffers."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:39
#, no-wrap
msgid "B<--getalignoff>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:41
msgid "Get alignment offset."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:41
#, no-wrap
msgid "B<--getbsz>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:43
msgid "Print blocksize in bytes."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:43
#, no-wrap
msgid "B<--getdiscardzeroes>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:45
msgid "Get discard zeroes support status."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:45
#, no-wrap
msgid "B<--getfra>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:47
msgid "Get filesystem readahead in 512-byte sectors."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:47
#, no-wrap
msgid "B<--getiomin>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:49
msgid "Get minimum I/O size."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:49
#, no-wrap
msgid "B<--getioopt>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:51
msgid "Get optimal I/O size."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:51
#, no-wrap
msgid "B<--getmaxsect>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:53
msgid "Get max sectors per request"
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:53
#, no-wrap
msgid "B<--getpbsz>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:55
msgid "Get physical block (sector) size."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:55
#, no-wrap
msgid "B<--getra>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:57
msgid "Print readahead (in 512-byte sectors)."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:57
#, no-wrap
msgid "B<--getro>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:59
msgid "Get read-only. Print 1 if the device is read-only, 0 otherwise."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:59
#, no-wrap
msgid "B<--getsize64>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:61
msgid "Print device size in bytes."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:61
#, no-wrap
msgid "B<--getsize>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:63
msgid ""
"Print device size (32-bit!) in sectors. Deprecated in favor of the --getsz "
"option."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:63
#, no-wrap
msgid "B<--getss>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:65
msgid "Print sectorsize in bytes \\(en usually 512."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:65
#, no-wrap
msgid "B<--getsz>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:67
msgid "Get size in 512-byte sectors."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:67
#, no-wrap
msgid "B<--rereadpt>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:69
msgid "Reread partition table"
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:69
#, no-wrap
msgid "B<--setbsz> I<bytes>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:73
msgid ""
"Set blocksize. Note that the block size is specific to the current file "
"descriptor opening the block device, so the change of block size only "
"persists for as long as blockdev has the device open, and is lost once "
"blockdev exits."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:73
#, no-wrap
msgid "B<--setfra> I<sectors>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:75
msgid "Set filesystem readahead (same like --setra on 2.6 kernels)."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:75
#, no-wrap
msgid "B<--setra> I<sectors>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:77
msgid "Set readahead (in 512-byte sectors)."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:77
#, no-wrap
msgid "B<--setro>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:79
msgid "Set read-only."
msgstr ""

#. type: IP
#: raw/man8/blockdev.8:79
#, no-wrap
msgid "B<--setrw>"
msgstr ""

#. type: Plain text
#: raw/man8/blockdev.8:81
msgid "Set read-write."
msgstr ""

