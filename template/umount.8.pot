msgstr ""

#. type: Plain text
#: raw/man8/mount.8:3186 raw/man8/umount.8:186
msgid "table of mounted filesystems"
msgstr ""

#. type: Plain text
#: raw/man8/mount.8:3198 raw/man8/umount.8:192
msgid "overrides the default location of the fstab file (ignored for suid)"
msgstr ""

#. type: Plain text
