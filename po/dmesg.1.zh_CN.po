#: raw/man1/dmesg.1:45
#, no-wrap
msgid "B<-E>, B<--console-on>"
msgstr ""

#. type: Plain text
#: raw/man1/dmesg.1:47
msgid "Enable printing messages to the console."
msgstr ""

#. type: IP
#: raw/man1/dmesg.1:47
#, no-wrap
msgid "B<-e>, B<--reltime>"
msgstr ""

#. type: Plain text
#: raw/man1/dmesg.1:49
msgid "Display the local time and the delta in human-readable format."
